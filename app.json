{
  "expo": {
    "name": "BreakTheIce",
    "slug": "breaktheice",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Break The Ice uses your location to identify users near you.",
        "NSPhotoLibraryUsageDescription": "Break The Ice access photo library for users usage only to select photos for their profile."
      },
      "bundleIdentifier": "com.nguyening20.breaktheice"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "experiments": {
      "turboModules": false
    },
    "description": "Made breaking the ice even easier!"
  }
}
